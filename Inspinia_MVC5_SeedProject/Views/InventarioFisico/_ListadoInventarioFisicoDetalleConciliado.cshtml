@model IEnumerable<ERP_GMEDINA.Models.tbInventarioFisicoDetalle>
@using ERP_GMEDINA.Models

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Listado de Inventario Fisico Detalle</h5>
				
                </div>
                <div class="ibox-content">

<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.invfd_Cantidad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.invfd_CantidadSistema)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbUnidadMedida.uni_Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbProducto.prod_Codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbProducto.prod_Descripcion)
        </th>
        <th>
           Sobrante/Faltante
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.invfd_Cantidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.invfd_CantidadSistema)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbUnidadMedida.uni_Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbProducto.prod_Codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbProducto.prod_Descripcion)
        </td>
        @{ 
            decimal Suma = (decimal)item.invfd_Cantidad - (decimal)item.invfd_CantidadSistema;
        }
        <td>
            @Suma
        </td>
    </tr>
            }

</table>

                </div>
            </div>
        </div>
    </div>
 </div>




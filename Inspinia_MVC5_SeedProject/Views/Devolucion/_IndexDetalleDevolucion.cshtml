@model IEnumerable<ERP_GMEDINA.Models.tbDevolucionDetalle>
@using ERP_GMEDINA.Models

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">

                <div class="ibox-content">
                    <table class="table table table-striped  table-bordered" id="DataTable1">
                        <thead>
                            <tr>
                                <th>
                                    Producto
                                    @*@Html.DisplayNameFor(model => model.tbProducto.prod_Descripcion)*@
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.devd_CantidadProducto)
                                </th>
                                <th>
                                    Monto
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.devd_Descripcion)
                                </th>
                              
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.tbProducto.prod_Descripcion)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.devd_CantidadProducto)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.devd_Monto)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.devd_Descripcion)
                                    </td>
                                  
                                </tr>
                            }
                        </tbody>
                    </table>

                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/jquery-3.3.1.js"></script>
<script src="~/Scripts/app/DataTable1.js"></script>



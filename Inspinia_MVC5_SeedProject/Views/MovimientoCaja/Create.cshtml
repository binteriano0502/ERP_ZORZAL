@model ERP_GMEDINA.Models.tbMovimientoCaja

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .mt-0 {
     margin-top: 0 !important;
}
</style>
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Arqueo Caja</h2>

    </div>
    <div class="col-sm-8">
        <div class="title-action">
            <a href="@Url.Action("Index","MovimientoCaja")">Regresar</a>
        </div>
    </div>
</div>



<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Registrar Arqueo Caja</h5>
                </div>
                <div class="ibox-content">

				@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        @Html.ValidationSummary(true)

        <div class="form-group">
            @Html.LabelFor(model => model.cja_Id, "Caja", new { @class = "control-label col-md-2" })
            <div class="col-md-3">
                @Html.EditorFor(model => model.cja_Id, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                @Html.ValidationMessageFor(model => model.cja_Id)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.mocja_UsuarioApertura, new { @class = "control-label col-md-2" })
            <div class="col-md-4">
                @Html.EditorFor(model => model.mocja_UsuarioApertura)
                @Html.ValidationMessageFor(model => model.mocja_UsuarioApertura)
            </div>
            @Html.LabelFor(model => model.mocja_FechaApertura, new { @class = "control-label col-md-2" })
            <div class="col-md-4">
                @Html.EditorFor(model => model.mocja_FechaApertura, "{0:yyyy-MM-dd}", new { htmlAttributes = new { @class = "form-control", @id = "fechaapertura" } })
                @Html.ValidationMessageFor(model => model.mocja_FechaApertura)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.mocja_UsuarioArquea, new { @class = "control-label col-md-2" })
            <div class="col-md-4">
                @Html.EditorFor(model => model.mocja_UsuarioArquea)
                @Html.ValidationMessageFor(model => model.mocja_UsuarioArquea)
            </div>
            @Html.LabelFor(model => model.mocja_FechaArqueo, new { @class = "control-label col-md-2" })
            <div class="col-md-4">
                @Html.EditorFor(model => model.mocja_FechaArqueo, "{0:yyyy-MM-dd}", new { htmlAttributes = new { @class = "form-control", @id = "fechaarqueo" } })
                @Html.ValidationMessageFor(model => model.mocja_FechaArqueo)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.mocja_UsuarioAceptacion, new { @class = "control-label col-md-2" })
            <div class="col-md-4">
                @Html.EditorFor(model => model.mocja_UsuarioAceptacion)
                @Html.ValidationMessageFor(model => model.mocja_UsuarioAceptacion)
            </div>
            @Html.LabelFor(model => model.mocja_FechaAceptacion, new { @class = "control-label col-md-2" })
            <div class="col-md-4">
                @Html.EditorFor(model => model.mocja_FechaAceptacion, "{0:yyyy-MM-dd}", new { htmlAttributes = new { @class = "form-control", @id = "fechaaceptacion" } })
                @Html.ValidationMessageFor(model => model.mocja_FechaAceptacion)
            </div>            
        </div>

        <div style="width:50%; float:left;">
            <div class="form-group m-t-xl">
                <div class="ibox-title">
                    <h5>Control de Efectivo</h5>
                </div>


                <label class="control-label col-md-4">Efectivo Inicial</label>
                <div class="control-label col-md-4">
                    <input type="text" value="" class="form-control" readonly />
                </div>
            </div>

            <div class="form-group m-t-n-xs">
                <label class="control-label col-md-4">Efectivo Recibido</label>
                <div class="control-label col-md-4">
                    <input type="text" value="" class="form-control" readonly />
                </div>
            </div>

            <div class="form-group m-t-n-xs">
                <label class="control-label col-md-4">Efectivo Pagos</label>
                <div class="control-label col-md-4">
                    <input type="text" value="" class="form-control" readonly />
                </div>
            </div>

            <div class="form-group m-t-n-xs">
                <label class="control-label col-md-4">Efectivo Entregado</label>
                <div class="control-label col-md-4">
                    <input type="text" value="" class="form-control" readonly />
                </div>
            </div>

            <div class="form-group m-t-n-xs">
                <label class="control-label col-md-4">Total Efectivo</label>
                <div class="control-label col-md-4">
                    <input type="text" value="" class="form-control" readonly />
                </div>
            </div>
        </div>



        <div style="width:50%; float:right;">
            <div class="form-group m-t-xl">
                <div class="ibox-title">
                    <h5>Control de Pagos</h5>
                </div>

                <label class="control-label col-md-4">Pagos Efectivo</label>
                <div class="control-label col-md-4">
                    <input type="text" value="" class="form-control" readonly />
                </div>
            </div>

            <div class="form-group m-t-n-xs">
                <label class="control-label col-md-4">Pagos TC o TD</label>
                <div class="control-label col-md-4">
                    <input type="text" value="" class="form-control" readonly />
                </div>
            </div>

            <div class="form-group m-t-n-xs">
                <label class="control-label col-md-4">Pagos Cheque</label>
                <div class="control-label col-md-4">
                    <input type="text" value="" class="form-control" readonly />
                </div>
            </div>

            <div class="form-group m-t-n-xs">
                <label class="control-label col-md-4">Pagos Nota de Credito</label>
                <div class="control-label col-md-4">
                    <input type="text" value="" class="form-control" readonly />
                </div>
            </div>

            <div class="form-group m-t-n-xs">
                <label class="control-label col-md-4">Pagos Cupón</label>
                <div class="control-label col-md-4">
                    <input type="text" value="" class="form-control" readonly />
                </div>
            </div>

            <div class="form-group m-t-n-xs">
                <label class="control-label col-md-4">Total Pagos</label>
                <div class="control-label col-md-4">
                    <input type="text" value="" class="form-control" readonly />
                </div>
            </div>
        </div>


        <div class="form-group m-t-xl">
            <div class=" col-md-12">
                <label class="control-label">Detalle Denomonaciòn Efectivo</label><br /><br />
                <table class="table table-bordered">

                    <tr>
                        <th class="success">Moneda</th>
                        <th class="success">Tipo Denominacion</th>
                        <th class="success">Denominacion</th>
                        <th class="success">Valor Denominacion</th>
                        <th class="success">Cantidad</th>
                        <th class="success">Valor</th>
                    </tr>
                    <tr>
                        <td>L.</td>
                        <td>Billete</td>
                        <td>500 Lempiras</td>
                        <td>500</td>
                        <td>10</td>
                        <td>5,000</td>
                    </tr>
                </table>
            </div>

        </div>

        <div class="form-group">
            <div class="col-md-offset-5 col-md-10">
                <input type="submit" value="Guardar" class="btn btn-primary" />
				@Html.ActionLink("Cancelar", "Index", null, new { @class = "btn btn-white"})
            </div>
        </div>
    </div>
}

                </div>
            </div>
        </div>
    </div>
 </div>

 

<script src="~/Scripts/jquery-3.3.1.js"></script>
<script src="~/Scripts/app/Datepicker.js"></script>

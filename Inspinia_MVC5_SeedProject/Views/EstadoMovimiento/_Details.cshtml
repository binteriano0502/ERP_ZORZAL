
@model ERP_GMEDINA.Models.tbEstadoMovimiento

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Estado Movimiento</h2>
        <ol class="breadcrumb"></ol>
    </div>
</div>




<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Detalle Estado Movimiento</h5>
                </div>
                <div class="ibox-content">

                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.estm_Id)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.estm_Id)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.estm_Descripcion)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.estm_Descripcion)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.estm_UsuarioCrea)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.estm_UsuarioCrea)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.estm_FechaCrea)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.estm_FechaCrea)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.estm_UsuarioModifica)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.estm_UsuarioModifica)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.estm_FechaModifica)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.estm_FechaModifica)
                        </dd>

                    </dl>


                    @Html.ActionLink("Editar", "Edit", new { id = Model.estm_Id }, new { @class = "btn btn-primary" })
                    @Html.ActionLink("Regresar al Listado", "Index", null, new { @class = "btn btn-white" })

                </div>
            </div>
        </div>
    </div>
</div>


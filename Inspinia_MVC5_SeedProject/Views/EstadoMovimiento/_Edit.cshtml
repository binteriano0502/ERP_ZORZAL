
@model ERP_GMEDINA.Models.tbEstadoMovimiento

@{
    ViewBag.Title = "Editar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Estado Movimiento</h2>
        <ol class="breadcrumb"></ol>
    </div>
    <div class="col-sm-8">
        <div class="title-action">
            @Html.ActionLink("Regresar al Listado", "Index", null, new { @class = "" })
        </div>
    </div>
</div>




<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Editar Estado Movimiento</h5>
                </div>
                <div class="ibox-content">

                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()

                        <div class="form-horizontal">

                            @Html.ValidationSummary(true)
                            @Html.HiddenFor(model => model.estm_Id)


                            <div class="form-group">
                                @Html.LabelFor(model => model.estm_Id, new { @class = "control-label col-md-2", @readonly = "true" })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.estm_Id)
                                    @Html.ValidationMessageFor(model => model.estm_Id)
                                </div>
                            </div>

                            <div class="form-group">
                                @Html.LabelFor(model => model.estm_Descripcion, new { @class = "control-label col-md-2" })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.estm_Descripcion)
                                    @Html.ValidationMessageFor(model => model.estm_Descripcion)
                                </div>
                            </div>

                            <div class="form-group">
                                @Html.LabelFor(model => model.estm_UsuarioCrea, new { @class = "control-label col-md-2", @readonly = "true" })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.estm_UsuarioCrea)
                                    @Html.ValidationMessageFor(model => model.estm_UsuarioCrea)
                                </div>
                            </div>

                            <div class="form-group">
                                @Html.LabelFor(model => model.estm_FechaCrea, new { @class = "control-label col-md-2", @readonly = "true" })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.estm_FechaCrea)
                                    @Html.ValidationMessageFor(model => model.estm_FechaCrea)
                                </div>
                            </div>

                            <div class="form-group">
                                @Html.LabelFor(model => model.estm_UsuarioModifica, new { @class = "control-label col-md-2", @readonly = "true" })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.estm_UsuarioModifica)
                                    @Html.ValidationMessageFor(model => model.estm_UsuarioModifica)
                                </div>
                            </div>

                            <div class="form-group">
                                @Html.LabelFor(model => model.estm_FechaModifica, new { @class = "control-label col-md-2", @readonly = "true" })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.estm_FechaModifica)
                                    @Html.ValidationMessageFor(model => model.estm_FechaModifica)
                                </div>
                            </div>

                            <div class="form-group">
                                <div class="col-md-offset-2 col-md-10">
                                    <input type="submit" value="Guardar" class="btn btn-primary" />
                                    @Html.ActionLink("Cancelar", "Index", null, new { @class = "btn btn-white" })
                                </div>
                            </div>
                        </div>
                    }


                </div>
            </div>
        </div>
    </div>
</div>



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
